{
  "name": "cherryjuice",
  "private": true,
  "bin": "apps/server/dist/main.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:client": "pnpm run dev --filter @cherryjuice/react-client  ",
    "dev:server": "pnpm run dev --filter @cherryjuice/nest-server  ",
    "migration:run": "pnpm run typeorm:migration:run --filter @cherryjuice/nest-server  ",
    "ws": "node scripts/workspace-script.js",
    "build:libs": "pnpm ws -- build",
    "test:libs": "pnpm ws -- test -p",
    "types:libs": "pnpm ws -- types",
    "build:apps": "pnpm ws -- -a -p build",
    "start": "pnpm run start --filter @cherryjuice/nest-server ",
    "test:apps": "pnpm ws -- -a -p test",
    "test": "concurrently npm:test:*",
    "strip:deps": "node scripts/strip-dev-dependencies.js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss)\"",
    "prettier:validate": "pnpm prettier --list-different",
    "prettier:fix": "pnpm prettier --write",
    "eslint:validate": "pnpm workspaces run eslint",
    "eslint:validate-root": "pnpm eslint",
    "eslint:fix": "pnpm workspaces run lint --fix",
    "validate": "concurrently npm:eslint:validate npm:prettier:validate",
    "fix": "pnpm eslint:fix && prettier:fix",
    "commit": "git-cz",
    "dc:pull": "sudo docker-compose pull",
    "dc:rm": "sudo docker-compose rm -f",
    "dc:stop": "sudo docker-compose stop",
    "predc:up": "pnpm dc:pull && pnpm dc:stop && pnpm dc:rm",
    "dc:up": "sudo docker-compose up -d",
    "dc:down": "sudo docker-compose down",
    "storybook": "rimraf node_modules/.cache/storybook && start-storybook -c storybook -p 1237",
    "storybook:build": "build-storybook -c storybook -o storybook-static",
    "build:app": "pnpm i --frozen-lockfile=false && pnpm build:libs && pnpm build:apps",
    "build:docker": "pnpm build:app &&  pnpm strip:deps --dev --include-client --include-server && sudo docker image build -t \"${1:-cherryjuice}\" .",
    "setup:dev": "pnpm i && pnpm build:libs && pnpm types:libs",
    "build:bin": "rimraf bin && npx pkg@4.4.9 --out-path=bin .",
    "postbuild:bin": "./scripts/build-bin/download-node-binaries-linux.sh && ./scripts/build-bin/download-node-binaries-win.sh && ./scripts/build-bin/gzip-artifects.sh"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "version": "0.0.1",
  "description": "",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycnmhd/cherryjuice.git"
  },
  "devDependencies": {
    "@cherryjuice/rollup-plugin-svg": "^0.0.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "graphql-upload": "^11.0.0",
    "husky": "3.1.0",
    "jest": "27.0.6",
    "lint-staged": "^10.4.0",
    "node-sass": "4.14.1",
    "concurrently":"6.2.0",
    "postcss": "8.2.1",
    "prettier": "^2.3.2",
    "rollup": "^2.34.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "^0.25.3",
    "sass": "^1.23.7",
    "ts-jest": "^25.2.1",
    "tsc-alias": "^1.2.10",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "cherrytree"
  ],
  "author": "Yacine Mohamdi",
  "license": "ISC",
  "homepage": "https://github.com/ycnmhd/cherryjuice#readme",
  "dependencies": {
    "concurrently": "^6.2.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "resolutions": {
    "graphql-upload": "^11.0.0"
  },
  "optionalDependencies": {
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-storyshots": "^6.1.11",
    "@storybook/addon-storysource": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "storybook": "^6.1.11",
    "storybook-addon-designs": "^5.4.2",
    "storybook-mobile": "^0.1.28"
  },
  "pkg": {
    "assets": [
      "apps/client/dist/**/*",
      "apps/server/dist/templates/**/*",
      "apps/server/dist/migrations/**/*"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-win-x64"
    ]
  }
}
