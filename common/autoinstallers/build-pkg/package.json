{
  "name": "build-pkg",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "i": "pnpm i && rush update-autoinstaller --name build-pkg"
  },
  "dependencies": {
    "pkg": "5.3.0",
    "rimraf": "3.0.2"
  },
  "bin": "apps/server/build/main.js",
  "pkg": {
    "assets": [
      "apps/client/build/**/*",
      "apps/server/build/templates/**/*",
      "apps/server/build/migrations/**/*"
    ],
    "targets": [
      "node14-linux-x64"
    ]
  }
}
