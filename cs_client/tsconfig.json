{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "::sass-modules/*": ["./src/assets/styles/modules/*"],
      "::sass-modules": ["./src/assets/styles/modules/index.ts"],
      "::types/*": ["./types/*"],
      "::helpers/*": ["./src/helpers/*"],
      "::graphql/*": ["./src/graphql/*"],
      "::assets/*": ["./src/assets/*"],
      "::shared-components/*": ["./src/components/app/shared-components/*"],
      "::hooks/*": ["./src/hooks/*"],
      "::app/*": ["./src/components/app/*"],
      "::auth/*": ["./src/components/auth/*"],
      "::root/*": ["./src/components/*"],
      "::store/*": ["./src/store/*"],
      "::cypress/*": ["./cypress/*"]
    },
    "target": "esnext",
    "sourceMap": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
