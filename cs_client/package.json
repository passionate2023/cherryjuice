{
  "name": "cherryscript-react-client",
  "version": "0.1.0",
  "scripts": {
    "copy-static-assets": "./scripts/cp-assets.sh",
    "prewatch:liveserver": "yarn run copy-static-assets",
    "watch:liveserver": "parcel -p 1236 watch src/assets/index.html ",
    "prebuild": "yarn run copy-static-assets",
    "build": "parcel build src/assets/index.html --no-source-maps --no-cache",
    "postinstall": "cd ./scripts/patch-postcss-camel-case/ && ./copy.sh",
    "cypress:open": "yarn cypress open",
    "watch:server": "cd ../cs_server/ && yarn run watch:server ",
    "commit": "git-cz",
    "docker:build": "docker image build -t cs ..",
    "docker:run": "docker container run -it -p 1430:1430 -e PORT=1430 -e ctbFolders=/cs/ctb-samples cs",
    "test:client": "jest .",
    "test:server": "cd ../cs_server && jest",
    "test": "run-p test:client test:server && lint-staged"
  },
  "author": "Yacine Mohamdi",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@material/linear-progress": "^5.1.0",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.7",
    "graphql-tag": "^2.10.1",
    "npm-run-all": "^4.1.5",
    "re-resizable": "^6.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27"
  },
  "optionalDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/cypress": "^5.3.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/testing-library__cypress": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "commitizen": "^4.0.3",
    "cypress": "^4.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "3.1.0",
    "jest": "^25.1.0",
    "lint-staged": "8.2.1",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.0",
    "ts-jest": "^25.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cssnano": "^4.1.10",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-compress": "^1.1.0",
    "postcss-camel-case": "^1.0.1",
    "postcss-exclude-files": "^1.0.15",
    "postcss-modules": "^1.4.1",
    "sass": "^1.23.7",
    "typescript": "^3.8.3"
  },
  "main": "src/index.ts",
  "keywords": [
    "react",
    "parcel"
  ],
  "description": "",
  "alias": {
    "::sass-modules": "./src/assets/styles/modules/",
    "::types": "./types/",
    "::helpers": "./src/helpers/",
    "::graphql": "./src/graphql/",
    "::assets": "./src/assets/",
    "::shared-components": "./src/components/app/shared-components/",
    "::hooks": "./src/hooks/",
    "::app": "./src/components/app/"
  },
  "sass": {
    "includePaths": [
      "./node_modules/",
      "./src/assets/styles"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "compress": {
    "gzip": {
      "enabled": false
    }
  }
}
