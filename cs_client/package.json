{
  "name": "parcel-express-react-client",
  "version": "0.1.0",
  "scripts": {
    "copy-static-assets": "./scripts/copy-static-assets.sh",
    "prewatch:liveserver": "yarn run copy-static-assets",
    "watch:liveserver": "parcel -p 1236 watch src/assets/index.html ",
    "watch": "parcel watch src/assets/index.html ",
    "prewatch:build": "copy-static-assets",
    "build": "parcel build src/assets/index.html"
  },
  "author": "Yacine Mohamdi",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material/linear-progress": "^4.0.0",
    "@types/jest": "^25.1.3",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.7",
    "graphql-tag": "^2.10.1",
    "jest": "^25.1.0",
    "ramda": "^0.26.1",
    "re-resizable": "^6.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-top-loading-bar": "^1.2.0",
    "ts-jest": "^25.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.5",
    "@types/ramda": "^0.26.38",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "postcss-camel-case": "^1.0.1",
    "postcss-exclude-files": "^1.0.15",
    "postcss-modules": "^1.4.1",
    "sass": "^1.23.7",
    "typescript": "^3.7.4"
  },
  "main": "src/index.ts",
  "keywords": [
    "react",
    "parcel"
  ],
  "description": "",
  "alias": {
    "::sass-modules": "./src/assets/styles/modules/",
    "::types": "./types/",
    "::helpers": "./src/helpers/",
    "::graphql": "./src/graphql/",
    "::assets": "./src/assets/",
    "::shared-components": "./src/components/shared-components/",
    "::hooks": "./src/hooks/",
    "::app": "./src/components/app/"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  }
}
