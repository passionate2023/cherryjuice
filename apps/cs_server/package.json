{
  "name": "@cherryjuice/nest-server",
  "workspaces": {
    "nohoist": [
      "typeorm/**",
      "typeorm"
    ]
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nest build --watch --webpack --webpackPath \"webpack.config.js\" ",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "test": "jest",
    "prestart": "yarn typeorm:migration:run",
    "start": "node dist/src/main.js",
    "lint": "eslint ./**",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.config.ts",
    "typeorm:migration:generate": "yarn typeorm migration:generate -- -n",
    "typeorm:migration:run": "yarn typeorm migration:run",
    "typeorm:migration:revert": "yarn typeorm migration:revert"
  },
